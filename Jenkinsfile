pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            } 
            
        }
        stage('Build') {
            steps {
                echo 'Hello World, I am building'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Hello World, I am deploying'
            }
        }
        stage('Test') {
            steps {
                echo 'Hello World, I am testing'
            }
        }
        stage('Release') {
            steps {
                echo 'Hello World, I am releasing'
            }
        }
    }
}
